<div class="panel-body transaction_details">
	<div class="panel-body transaction_details">
		<h4>Your Public Bitcoin Addresses:</h4>
		<% @public_address.each do |key| %>
			<% @useraddress = key["address"]["address"] %>
			<% @url = "https://blockchain.info/address/" + @useraddress %>
			<%= link_to @useraddress, @url %>
		<% end %>
	</div>
</div>